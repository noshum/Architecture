<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeActivity">

    <Button
        android:layout_width="@dimen/dimens_150dp"
        android:layout_height="wrap_content"
        android:text="No Pattern"
        app:layout_constraintBottom_toTopOf="@+id/btnMVP"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:id="@+id/btnNoPattern"/>

    <Button
        android:layout_width="@dimen/dimens_150dp"
        android:layout_height="wrap_content"
        android:text="MVP"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:id="@+id/btnMVP"/>


    <Button
        android:layout_width="@dimen/dimens_150dp"
        android:layout_height="wrap_content"
        android:text="MVVM"
        app:layout_constraintTop_toBottomOf="@+id/btnMVP"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:id="@+id/btnMVVM"/>


</androidx.constraintlayout.widget.ConstraintLayout>